ck={() => {\r\n\r\n                        // Remove Details                        \r\n                        let removedDetailsArr = productDetails.filter(data => data.uid != record.uid);\r\n                        setProductDetails(removedDetailsArr);\r\n                    }}\r\n                />\r\n            )\r\n        }\r\n\r\n    ];\r\n\r\n    const productDetailTableData = productDetails.map((data, i) => ({\r\n        key: i,\r\n        details: data.productDetail,\r\n        uid: data.uid\r\n    }));\r\n\r\n    return (\r\n        <>\r\n            <h1 style={{ margin: \"30px 0px\" }}>Add Product Details</h1>\r\n            <Row style={{ margin: \"30px 0px\" }}>\r\n\r\n                <Col\r\n                    md={{ span: 18, offset: 2 }}\r\n                    xs={24}\r\n                >                    \r\n                        <Input\r\n                            ref={detailInputRef}\r\n                            onChange={() => {\r\n                                detailInputRef.current.input.style.border = \"1px solid #d9d9d9\";\r\n                            }}\r\n                        />\r\n                </Col>\r\n                <Col\r\n                    md={4}\r\n                    xs={24}\r\n                >\r\n                    <Button\r\n                        type=\"primary\"\r\n                        onClick={handleAddDetail}\r\n                    >Add Detail</Button>\r\n                </Col>\r\n            </Row>\r\n            <Table\r\n                columns={productDetailColumns}\r\n                bordered={true}\r\n                dataSource={productDetailTableData}\r\n            />\r\n        </>\r\n    );\r\n}\r\n\r\nexport default ProductDetailTable;","E:\\FYP\\geo-ecommerce\\src\\pages\\sell_your_products\\components\\sell_products_tab\\uploadImageComponent.js",["245","246","247"],"import React, { useState, useEffect } from 'react';\r\nimport { Table, Upload, message, Button } from 'antd';\r\nimport { 